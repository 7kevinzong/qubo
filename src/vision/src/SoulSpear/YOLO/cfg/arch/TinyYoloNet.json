{
    "model" : {
      "width": 416,
      "height": 416,
      "num_classes": 20,
      "anchors" : [1.08,1.19,3.42,4.41,6.63,11.38,9.42,5.11,16.62,10.52]
    },

    "checkpoint":{
      "root" : "~/SoulSpear/YOLO/checkpoint/TinyYoloNet",
